<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tw.com.rex.leave_system.mapper.CompanyFaxMapper">
  <resultMap id="BaseResultMap" type="tw.com.rex.leave_system.model.dao.CompanyFax">
    <id column="fax" jdbcType="VARCHAR" property="fax" />
    <id column="company_id" jdbcType="VARCHAR" property="companyId" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="map">
    delete from company_fax
    where fax = #{fax,jdbcType=VARCHAR}
      and company_id = #{companyId,jdbcType=VARCHAR}
  </delete>
  <insert id="insert" parameterType="tw.com.rex.leave_system.model.dao.CompanyFax">
    insert into company_fax (fax, company_id)
    values (#{fax,jdbcType=VARCHAR}, #{companyId,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select fax, company_id
    from company_fax
  </select>
</mapper>