<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tw.com.rex.leave_system.mapper.CompanyMobileMapper">
  <resultMap id="BaseResultMap" type="tw.com.rex.leave_system.model.dao.CompanyMobile">
    <id column="mobile" jdbcType="VARCHAR" property="mobile" />
    <id column="company_id" jdbcType="VARCHAR" property="companyId" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="map">
    delete from company_mobile
    where mobile = #{mobile,jdbcType=VARCHAR}
      and company_id = #{companyId,jdbcType=VARCHAR}
  </delete>
  <insert id="insert" parameterType="tw.com.rex.leave_system.model.dao.CompanyMobile">
    insert into company_mobile (mobile, company_id)
    values (#{mobile,jdbcType=VARCHAR}, #{companyId,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select mobile, company_id
    from company_mobile
  </select>
</mapper>