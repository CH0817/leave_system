<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tw.com.rex.leave_system.mapper.EmployeeEmailMapper">
  <resultMap id="BaseResultMap" type="tw.com.rex.leave_system.model.dao.part.employee.EmployeeEmail">
    <id column="email" jdbcType="VARCHAR" property="email" />
    <id column="employee_id" jdbcType="VARCHAR" property="employeeId" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="map">
    delete from employee_email
    where email = #{email,jdbcType=VARCHAR}
      and employee_id = #{employeeId,jdbcType=VARCHAR}
  </delete>
  <insert id="insert" parameterType="tw.com.rex.leave_system.model.dao.part.employee.EmployeeEmail">
    insert into employee_email (email, employee_id)
    values (#{email,jdbcType=VARCHAR}, #{employeeId,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select email, employee_id
    from employee_email
  </select>
</mapper>