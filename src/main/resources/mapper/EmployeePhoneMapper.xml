<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tw.com.rex.leave_system.mapper.EmployeePhoneMapper">
  <resultMap id="BaseResultMap" type="tw.com.rex.leave_system.model.dao.EmployeePhone">
    <id column="phone" jdbcType="VARCHAR" property="phone" />
    <id column="employee_id" jdbcType="VARCHAR" property="employeeId" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="map">
    delete from employee_phone
    where phone = #{phone,jdbcType=VARCHAR}
      and employee_id = #{employeeId,jdbcType=VARCHAR}
  </delete>
  <insert id="insert" parameterType="tw.com.rex.leave_system.model.dao.EmployeePhone">
    insert into employee_phone (phone, employee_id)
    values (#{phone,jdbcType=VARCHAR}, #{employeeId,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select phone, employee_id
    from employee_phone
  </select>
</mapper>